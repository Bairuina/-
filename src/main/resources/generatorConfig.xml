<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="Mybatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://39.106.116.74:3306/Purchase?autoReconnect=true;useUnicode=true;characterEncoding=utf8"
                        userId="root"
                        password="wyxnb">
        </jdbcConnection>
        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
                   NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="true" />
        </javaTypeResolver>

        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="net.wlgzs.purchase.entity"
                            targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true" />
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- 指定SQL映射文件生成的位置 ，记得修改namespace-->
        <!--<sqlMapGenerator targetPackage="mappers" targetProject=".\src\main\resources">-->
        <!--<property name="enableSubPackages" value="true"/>-->
        <!--</sqlMapGenerator>-->
        <!--<javaClientGenerator type="XMLMAPPER" targetPackage="org.wlgzs.hospitalmanage.dao" targetProject=".\src\main\java">-->
        <!--<property name="enableSubPackages" value="true"/>-->
        <!--</javaClientGenerator>-->

        <table tableName="parameter" domainObjectName="Parameter"></table>
        <table tableName="parts" domainObjectName="Parts"></table>
        <table tableName="product" domainObjectName="Product"></table>
        <table tableName="service" domainObjectName="Service"></table>
        <table tableName="user" domainObjectName="User"></table>

    </context>
</generatorConfiguration>